<?xml version="1.0"?>
<xs:schema
	targetNamespace="http://www.vdab.be/xmlcursus/alcohol"
		xmlns:xs="http://www.w3.org/2001/XMLSchema"
		xmlns="http://www.vdab.be/xmlcursus/alcohol"
		elementFormDefault="qualified">

    <xs:simpleType name="AlcoholPercentageType">
        <xs:restriction base="xs:decimal">
            <xs:minInclusive value="0"/>
			<xs:maxInclusive value="16"/>
        </xs:restriction>
    </xs:simpleType>
	<xs:element name="AlcoholPercentage" type="AlcoholPercentageType"/>
</xs:schema>