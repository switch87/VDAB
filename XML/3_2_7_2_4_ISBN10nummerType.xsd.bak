<?xml version="1.0"?>
<xs:schema
	targetNamespace="http://www.vdab.be/xmlcursus/oefalcperc"
		xmlns:xs="http://www.w3.org/2001/XMLSchema"
		xmlns="http://www.vdab.be/xmlcursus/oefalcperc"
		elementFormDefault="qualified">

    <xs:simpleType name="ISBN10nummerType">
        <xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{9}[a-zA-Z0-9]"/>
        </xs:restriction>
    </xs:simpleType>
	<xs:element name="ISBN10nummer" type="ISBN10nummerType"/>

</xs:schema>